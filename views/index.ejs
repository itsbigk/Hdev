<!doctype html>
<!-- adding the angular app to the page -->
<html ng-app="homeless">
<head>
  <title>Homeless Devices</title>

  <!-- jQuery library. this goes before bootstrap because bootstrap's js file requires jQuery -->
  <script src="/lib/jquery/dist/jquery.min.js"></script>

  <!-- bootstrap -->
  <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css">
  <script type="text/javascript" src="/lib/bootstrap/dist/js/bootstrap.min.js"></script>

  <!-- font awesome -->
  <link rel="stylesheet" href="/lib/font-awesome/css/font-awesome.min.css">

  <!-- animate.css -->
  <link rel="stylesheet" href="/lib/animate.css/animate.min.css">

  <!-- local stylesheet -->
  <link rel="stylesheet" type="text/css" href="style.css">

  <!-- Angularjs -->
  <script src="/lib/angular/angular.min.js"></script>

  <!-- angular ui router -->
  <script src="/lib/angular-ui-router/release/angular-ui-router.min.js"></script>

  <!-- angular controllers and services -->
  <script src="/js/controllers/caseController.js"></script>
  <script src="/js/controllers/rootController.js"></script>
  <script src="/js/services/caseService.js"></script>
  <script src="/js/services/config.js"></script>

  <!-- local angular app file -->
  <script src="/js/app.js"></script>
</head>
<body>
  <div ng-include='"navbar.html"'></div>
  <div ui-view></div>
  <div ng-include='"footer.html"'></div>
</body>
</html>
